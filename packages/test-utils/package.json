{
	"name": "@rchat/test-utils",
	"version": "0.3.0",
	"sideEffects": false,
	"publishConfig": {
		"access": "public",
		"directory": "prepared-package"
	},
	"scripts": {
		"build": "aqu build",
		"build-storybook": "build-storybook",
		"dev": "aqu watch --no-cleanup",
		"lint": "eslint **/*.{ts,tsx}",
		"lint:fix": "eslint . --fix",
		"postpublish": "rimraf ./prepared-package",
		"prepublishOnly": "rimraf ./prepared-package && clean-publish",
		"release": "np",
		"start": "nodemon ./dist/index.js",
		"storybook": "start-storybook -p 6006",
		"test": "aqu test --passWithNoTests",
		"test:watch": "aqu test --passWithNoTests --watch"
	},
	"dependencies": {
		"aqu": "^0.4.3",
		"socket.io": "^4.5.1",
		"socket.io-client": "^4.5.1"
	},
	"devDependencies": {
		"@rchat/server": "workspace:^0.3.0",
		"nodemon": "^2.0.20",
		"rimraf": "^3.0.2"
	}
}
