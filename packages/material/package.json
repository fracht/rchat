{
	"name": "@rchat/material",
	"version": "0.11.2",
	"sideEffects": false,
	"main": "./dist/src/index.js",
	"types": "./dist/src/index.d.ts",
	"publishConfig": {
		"access": "public",
		"directory": "prepared-package"
	},
	"scripts": {
		"build": "tsc",
		"build-storybook": "build-storybook",
		"dev": "tsc --watch",
		"lint": "eslint **/*.{ts,tsx}",
		"lint:fix": "eslint . --fix",
		"postpublish": "rimraf ./prepared-package",
		"prepublishOnly": "rimraf ./prepared-package && clean-publish",
		"release": "np",
		"storybook": "cross-env NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006",
		"test": "aqu test --passWithNoTests",
		"test:watch": "aqu test --passWithNoTests --watch"
	},
	"dependencies": {
		"@mui/icons-material": "^5.10.6",
		"@mui/system": "^5.10.6"
	},
	"devDependencies": {
		"@babel/core": "^7.18.9",
		"@emotion/react": "^11.10.4",
		"@emotion/styled": "^11.10.4",
		"@mui/material": "^5.10.6",
		"@mui/styled-engine": "^5.10.6",
		"@mui/types": "^7.2.0",
		"@rchat/react": "workspace:^0.11.2",
		"@storybook/addon-actions": "^6.5.13",
		"@storybook/addon-essentials": "^6.5.13",
		"@storybook/addon-interactions": "^6.5.13",
		"@storybook/addon-links": "^6.5.13",
		"@storybook/builder-webpack4": "^6.5.13",
		"@storybook/manager-webpack4": "^6.5.13",
		"@storybook/react": "^6.5.13",
		"@storybook/testing-library": "^0.0.13",
		"@types/react": "^18.0.9",
		"aqu": "^0.4.3",
		"babel-loader": "^8.2.5",
		"cross-env": "^7.0.3",
		"react": "^18.1.0",
		"react-dom": "^18.1.0",
		"rimraf": "^3.0.2",
		"typescript": "^4.7.2"
	},
	"peerDependencies": {
		"@emotion/react": "^11.10.4",
		"@emotion/styled": "^11.10.4",
		"@mui/material": "^5.10.6",
		"@rchat/react": "< 1.0.0",
		"react": "^18.0.0",
		"react-dom": "^18.0.0"
	}
}
