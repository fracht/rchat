		"socket.io": "^4.5.1"
	},
	"devDependencies": {
		"@babel/core": "^7.18.2",
		"@types/jest": "^29.2.0",
		"@types/node": "^18.0.6",
		"aqu": "0.4.3",
		"jest": "^29.2.2",
		"jest-watch-typeahead": "^2.2.0",
		"rimraf": "^3.0.2",
		"typescript": "^5.0.4"
	}
