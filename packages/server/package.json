{
	"name": "@rchat/server",
	"version": "0.12.0",
	"main": "./dist/server.cjs.development.js",
	"types": "./dist/server.d.ts",
	"publishConfig": {
		"access": "public",
		"directory": "prepared-package"
	},
	"scripts": {
		"build": "aqu build && rimraf ./prepared-package && clean-publish",
		"lint": "eslint **/*.{ts,tsx}",
